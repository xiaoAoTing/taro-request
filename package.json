{
  "name": "@lightgreen/taro-request",
  "version": "1.0.0",
  "description": "A lightweight HTTP request library for Taro framework with interceptors support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "taro",
    "request",
    "http",
    "axios",
    "interceptor",
    "wechat",
    "miniprogram"
  ],
  "author": "lightgreen",
  "license": "MIT",
  "peerDependencies": {
    "@tarojs/taro": "^3.6.0"
  },
  "devDependencies": {
    "@tarojs/taro": "^3.6.24",
    "@types/node": "^18.15.11",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lightgreen/taro-request.git"
  }
}

