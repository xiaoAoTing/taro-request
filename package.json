{
  "name": "@lightgreen/taro-request",
  "version": "1.0.0",
  "description": "A lightweight HTTP request library for Taro3 with axios-style API",
  "main": "dist/request.js",
  "module": "dist/request.mjs",
  "types": "dist/request.d.ts",
  "exports": {
    ".": {
      "import": "./dist/request.mjs",
      "require": "./dist/request.js",
      "types": "./dist/request.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "taro",
    "taro3",
    "request",
    "http",
    "axios",
    "interceptor"
  ],
  "author": "lightgreen",
  "license": "MIT",
  "peerDependencies": {
    "@tarojs/taro": "^3.6.0"
  },
  "devDependencies": {
    "@tarojs/taro": "^3.6.24",
    "@types/node": "^18.15.11",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoAoTing/taro-request.git"
  }
}

